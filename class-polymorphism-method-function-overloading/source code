class Sivakumar{
    public:
        void act(){
            cout<<"Sivakumar is acting";}};
class Surya:public Sivakumar{
    public: 
        void act(){
            cout<<"Surya is acting";}};
int main(){
    Surya s1;
    s1.act();//over-riding 
}



